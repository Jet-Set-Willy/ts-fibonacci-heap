  },
  "homepage": "https://github.com/gwtw/ts-fibonacci-heap",
  "devDependencies": {
    "@tyriar/heap-tests": "^1.0.2",
    "ava": "^0.24.0",
    "coveralls": "^3.0.0",
    "nyc": "^11.4.1",
    "tslint": "^5.9.1",
    "tslint-consistent-codestyle": "^1.13.0",
    "typescript": "^2.8.3"
  },
  "files": [
    "lib/*.js",
